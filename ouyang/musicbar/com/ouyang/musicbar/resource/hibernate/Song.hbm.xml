<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
     "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
    <hibernate-mapping>
    	<class table="tb_mb_song" name="com.ouyang.musicbar.domain.Song">
    		<id name="id" type="java.lang.String" column="ID" length="30">
    			<generator class="sequence">
    				<param name="sequence">TB_MB_SONG_SEQUENCE</param>
    			</generator>
    		</id>
    		<property name="songName" type="java.lang.String" column="SONG_NAME" length="30"/>
    		<property name="singerName" type="java.lang.String" column="SINGER_NAME" length="30"/>
    		<property name="specialName" type="java.lang.String" column="SPECIAL_NAME" length="30"/>
    		<property name="fileSize" type="java.lang.String" column="FILE_SIZE" length="30"/>
    		<property name="fileURL" type="java.lang.String" column="FILE_URL" length="30"/>
    		<property name="format" type="java.lang.String" column="FORMAT" length="30"/>
    		<property name="hits" type="java.lang.Integer" column="HITS" length="30"/>
    		<property name="download" type="java.lang.Integer" column="DOWNLOAD" length="30"/>
    		<property name="upTime" type="java.util.Date" column="UP_TIME"/>
    		<property name="playId" type="java.lang.String" column="PLAY_ID" length="30"/>
    		<property name="songTypeId" type="java.lang.String" column="SONG_TYPE_ID" length="30"/>
    		<property name="songType" type="java.lang.String" column="SONG_TYPE" length="30"/>
    		<property name="songNameShort" type="java.lang.String" column="SONG_NAME_SHORT" length="30"/>
    	</class>
    </hibernate-mapping>